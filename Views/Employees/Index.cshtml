@model List<EmployeePortal.Models.Domain.Employee>
@{
}

<h1>Employee</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Sallary</th>
            <th>Date Of Birth</th>
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>@employee.Sallary</td>
                <td>@employee.DateOfBirth.ToString("dd-MM-yyyyy")</td>
                <td>@employee.Department</td>
                <td><a href ="Employees/View/@employee.Id">View</a></td>
            </tr>
            
        }
    </tbody>
</table>